1 DOS F:POKE 150,7
2 ON BRK GOTO 5000
5 WIDTH 40
10 CLS
15 CLEAR 10000:DIM P$(128)
16 PRINT"        ASSEMBLY PRINTER"
20 PRINT"   (C) 1989 BY JEREMY SPILLER"
30 PRINT
50 LINE INPUT"FILE NAME? ";F$
60 LINE INPUT"PROGRAM NAME? ";P$
80 P=0:OPEN "I",#1,F$
90 GOSUB 1000
100 X=X+1:LINE INPUT#1,P$(X):IF X<118 AND NOT(EOF(1)) THEN 100
110 GOSUB 2000:IF NOT(EOF(1)) THEN 90
120 CLOSE:PRINT"DONE"
140 STOP
1000 FORX=1TO118:P$(X)="":NEXT:X=0:RETURN
2000 REM PRINT HEADER
2010 P=P+1:H$=P$+" - PAGE"+STR$(P):PRINT#-2:PRINT#-2:PRINT#-2:PRINT#-2,STRING$(40-LEN(H$)/2," ")+H$:PRINT#-2
2020 FORX=1TO59
2030 PRINT#-2,"      ;";:IF LEN(P$(X))>32 THEN PRINT#-2,LEFT$(P$(X),32); :ELSE PRINT#-2,P$(X)+STRING$(32-LEN(P$(X))," ");";";
2050 IF LEN(P$(X+59))>32 THEN PRINT#-2,LEFT$(P$(X+59),32) :ELSE PRINT#-2,P$(X+59)+STRING$(32-LEN(P$(X+59))," ")
2060 NEXT X:PRINT#-2:PRINT#-2:RETURN
5000 PRINT#-2:PRINT"BREAK":STOP
