1 JMS F
5 PCLEAR 4
10 CLEAR 200,&H3FF0
20 INPUT"LEVEL";A$
30 LOADM"OBJECT "+A$
35 INPUT "INSERT CRYSTAL DISK, AND HIT    ENTER";Q$
39 Q=&H6001
40 FOR X=&H4000 TO &H6000 STEP 4
50 IF PEEK(X)=255 THEN POKE Q,255:POKE Q+1,255:POKE &H6000,(Q-&H6000)/256:SAVEM "OBJECT "+A$,&H6000,Q,0:POKE 113,0:DLOAD
60 A=PEEK(X)*256+PEEK(X+1):A=INT(A/2)+14:B=INT(A/256):C=A-B*256
70 POKE Q,B:POKE Q+1,C:Q=Q+2
75 A=PEEK(X+3)
80 ON A GOTO 100,200,300,400,500,600,700
90 IF A=0 THEN POKE Q,2:Q=Q+1 :ELSE PRINT "ERROR":STOP
91 GOTO 1000
100 POKE Q,4:Q=Q+1:GOTO 1000
200 POKE Q,10:Q=Q+1:GOTO 1000
300 POKE Q,8:Q=Q+1:GOTO 1000
400 POKE Q,12:Q=Q+1:GOTO 1000
500 POKE Q,14:Q=Q+1:GOTO 1000
600 POKE Q,16:Q=Q+1:GOTO 1000
700 POKE Q,18:Q=Q+1:GOTO 1000
1000 A=PEEK(X+2):POKE Q,A:Q=Q+1
1010 NEXT X
