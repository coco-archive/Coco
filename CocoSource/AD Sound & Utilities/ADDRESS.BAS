10 CLS
20 LINE INPUT"FILE: ";F$
30 OPEN "R",#1,F$,1
40 FIELD #1,1 AS A$
60 TRANS=1:TL=0
70 GET #1,TRANS
80 IF ASC(A$)=255 THEN PRINT:PRINT"EXEC ADDR @";:GET #1,TRANS+3:E=ASC(A$)*256:GET #1,TRANS+4:E=E+ASC(A$):PRINT HEX$(E),"  TL $";HEX$(TL+5):END
90 IF ASC(A$)<>0 THEN PRINT:PRINT"PREAMBLE/POSTAMBLE ERROR":END
100 GET #1,TRANS+1:L=ASC(A$)*256:GET #1,TRANS+2:L=L+ASC(A$)
110 GET #1,TRANS+3:LO=ASC(A$)*256:GET #1,TRANS+4:LO=LO+ASC(A$)
120 TRANS=TRANS+5:TL=TL+5
130 PRINT"START @";HEX$(LO),"LEN $";HEX$(L)
140 TRANS=TRANS+L:TL=TL+L
150 GOTO 70
