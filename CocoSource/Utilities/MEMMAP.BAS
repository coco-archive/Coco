1 JMS F:POKE 150,7
10 LINE INPUT "TITLE: ";T$
20 PRINT#-2,CHR$(27);"@";CHR$(27);"3";CHR$(36);::'44 LINES/INCH
30 PRINT#-2,STRING$(40-LEN(T$)/2," ");T$:PRINT#-2:PRINT#-2
35 PRINT#-2,CHR$(27);"-";CHR$(1);
40 FOR X=&H30 TO &H3F
50 PRINT#-2,"     ";RIGHT$("00"+HEX$(X),2);"  ";RIGHT$("00"+HEX$(X*2)+"000",5);" -";STRING$(80-18," ");"-"
60 NEXT X
70 PRINT#-2:PRINT#-2:PRINT#-2
85 :'PRINT VIRTUAL
90 FOR X=0 TO &HF
100 PRINT#-2,"    FFA";HEX$(X);"  ";RIGHT$(HEX$(X*2),1);"000 -";STRING$(80-18," ");"-"
110 IF X=7 THEN PRINT#-2:PRINT#-2
120 NEXT X
200 PRINT#-2,CHR$(27);"@"
